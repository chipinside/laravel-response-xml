{
    "name": "chipinside/laravel-response-xml",
    "description": "Add the method xml integrating the laravel's response, converting eloquent return to XML.",
    "keywords": ["xml", "laravel", "eloquent", "response"],
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/container": "^7.0|^8.0|^9.0",
        "illuminate/routing": "^7.0|^8.0|^9.0",
        "laravel/framework": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "XmlResponse\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "XmlResponse\\XmlResponseServiceProvider"
            ]
        }
    }
}
